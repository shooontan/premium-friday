{
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": true,
    "member-access": false,
    "no-string-literal": false,
    "no-implicit-dependencies": [
      true,
      [
        "mockdate"
      ]
    ]
  }
}
